{
  "name": "svmxc-ext",
  "description": "Extension plugin for ServiceMax SFDX plugin",
  "version": "0.0.1",
  "author": "Deepak.H.Andeli @deepakandeli",
  "bugs": "https://github.com/deepakandeli/svmxc-ext/issues",
  "dependencies": {
    "@oclif/core": "^1",
    "@salesforce/command": "^5",
    "@salesforce/core": "^3",
    "svmxc-sfdx": "^0.0.12",
    "tslib": "^2"
  },
  "devDependencies": {
    "@oclif/test": "^2",
    "@salesforce/ts-sinon": "^1",
    "@types/chai": "^4",
    "@types/mocha": "^8",
    "@typescript-eslint/eslint-plugin": "^4",
    "@typescript-eslint/parser": "^4",
    "chai": "^4",
    "eslint": "^7",
    "eslint-config-prettier": "^8",
    "eslint-plugin-header": "^3",
    "eslint-plugin-import": "^2",
    "eslint-plugin-jsdoc": "^35",
    "eslint-plugin-prettier": "^3",
    "eslint-plugin-typescript": "^0",
    "mocha": "^9",
    "nyc": "^15",
    "oclif": "^3",
    "prettier": "^2",
    "rimraf": "latest",
    "sinon": "10.0.0",
    "ts-node": "^10",
    "typescript": "4"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "files": [
    "/lib",
    "/messages",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/deepakandeli/svmxc-ext",
  "keywords": [
    "sfdx-plugin"
  ],
  "license": "MIT",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "sfdx",
    "topics": {
      "hello": {
        "description": "Commands to say hello."
      }
    }
  },
  "repository": "deepakandeli/svmxc-ext",
  "scripts": {
    "build": "tsc -p .",
    "lint": "eslint src/**/*.ts test/**/*.ts",
    "postpack": "rimraf oclif.manifest.json",
    "posttest": "eslint src/**/*.ts test/**/*.ts",
    "prepack": "rimraf lib && tsc -b && oclif manifest && oclif readme",
    "test": "nyc --extension .ts --require ts-node/register mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md"
  }
}
